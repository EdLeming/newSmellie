#!/usr/bin/env python
from config import PORT
from xmlrpclib import ServerProxy
import curses
import socket

s = ServerProxy('http://127.0.0.1:{0}'.format(PORT))

def get_param(str):
    return str.split(" ")

screen = curses.initscr()
try:
    while True:
        screen.clear()
        screen.border(0)

except KeyboardInterrupt:
    print "Exiting"

